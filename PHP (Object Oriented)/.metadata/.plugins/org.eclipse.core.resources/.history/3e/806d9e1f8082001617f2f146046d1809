<?php
class Login{
	private $email = "";
	private $password = "";
	
	function Login()
	{
		$email = "";
		$password = "";
	}
	function Login($email, $password)
	{
		$email = $email;
		$password = $password;
	}
	//get email from html
	function getEmailFromBoostrap()
	{
		return $_POST["email"];
	}
	//get password from html
	function getPasswordFromBoostrap()
	{
		return $_POST["password"];
	}
	
	function getEmail()
	{
		return $email;
	}
	function getPassword()
	{
		return $password;
	}
	function setEmail($email)
	{
		$email = $email;
	}
	function setPassword($password)
	{
		$password = $password;
	}
	
}
?>